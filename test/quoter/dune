(alias
 (name runtest)
 (deps
  (:test test.ml)
  (glob_files %{project_root}/src/.ppxlib.objs/byte/*.cmi))
 (action (chdir %{project_root}
          (progn
           (ignore-outputs
            (run %{project_root}/test/expect/expect_test.exe %{test}))
           (diff? %{test} %{test}.corrected)))))
